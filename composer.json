{
    "name": "morphose/morphose",
    "description": "A data humanizer for PHP humans.",
    "keywords": [
        "bytes",
        "convert",
        "converter",
        "humanize",
        "humanizer",
        "php"
    ],
    "license": "MIT",
    "homepage": "https://morphose.github.io",
    "support": {
        "issues": "https://github.com/morphose/morphose/issues",
        "source": "https://github.com/morphose/morphose"
    },
    "authors": [
        {
            "name": "Eduardo Agostini",
            "email": "edu.agostini@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "orchestra/testbench": "^3.4",
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Morphose\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Morphose\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
