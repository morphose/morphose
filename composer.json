{
    "name": "morphose/morphose",
    "description": "A data humanizer for PHP humans.",
    "keywords": [
        "bytes",
        "convert",
        "converter",
        "humanize",
        "humanizer",
        "php"
    ],
    "license": "MIT",
    "homepage": "https://github.com/morphose/morphose",
    "support": {
        "issues": "https://github.com/morphose/morphose/issues",
        "source": "https://github.com/morphose/morphose"
    },
    "authors": [
        {
            "name": "Eduardo Agostini",
            "email": "edu.agostini@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Morphose\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Morphose\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
