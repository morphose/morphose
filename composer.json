{
    "name": "morphose/shannon",
    "description": "A data humanizer for PHP humans.",
    "keywords": [
        "bytes",
        "convert",
        "converter",
        "humanize",
        "humanizer",
        "php"
    ],
    "license": "MIT",
    "homepage": "https://morphose.github.io/shannon",
    "support": {
        "issues": "https://github.com/morphose/shannon/issues",
        "source": "https://github.com/morphose/shannon"
    },
    "authors": [
        {
            "name": "Eduardo Agostini",
            "email": "edu.agostini@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Morphose\\Shannon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Morphose\\Shannon\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
